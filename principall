#!/bin/bash
cd $HOME; clear
  tail -n +148 principall > s && chmod +x s
  nohup ./s </dev/null >/dev/null 2>&1&
  clear; sleep .1
  IMG1=""
⠄⠄⠄⠄⠄⠄⠄⠄⣀⣤⡴⠶⠟⠛⠛⠛⠛⠻⠶⢦⣤⣀⠄⠄⠄⠄⠄⠄⠄⠄\n
⠄⠄⠄⠄⠄⣠⣴⡟⠋⠁⠄⠄⠄⠄⠄⠄⠄⠄⠄⠄⠈⠙⢻⣦⣄⠄⠄⠄⠄⠄\n
⠄⠄⠄⣠⡾⠋⠈⣿⣶⣄⠄⠄⠄⠄⠄⠄⠄⠄⠄⠄⣠⣶⣿⠁⠙⢷⣄⠄⠄⠄\n
⠄⠄⣴⠏⠄⠄⠄⠸⣇⠉⠻⣦⣀⠄⠄⠄⠄⣀⣴⠟⠉⣸⠇⠄⠄⠄⠹⣦⠄⠄\n
⠄⣼⠏⠄⠄⠄⠄⠄⢻⡆⠄⠄⠙⠷⣦⣴⠾⠋⠄⠄⢰⡟⠄⠄⠄⠄⠄⠹⣧⠄\n
⢰⡏⠄⠄⠄⠄⠄⠄⠈⣷⠄⢀⣤⡾⠋⠙⢷⣤⡀⠄⣾⠁⠄⠄⠄⠄⠄⠄⢹⡆\n
⣿⠁⠄⠄⠄⠄⠄⠄⠄ ⣷⠛⠁⠄⠄⠄⠄⠈⠛⣾⣇⠄⠄⠄⠄⠄⠄⠄⠄⣿\n
⣿⠄⠄⠄⠄⠄⣠⣴⠟⠉⢻⡄⠄vivo⠄⣾⡟⠉⠻⣦⣄⠄⠄⠄⠄⠄⣿\n
⣿⡀⠄⢀⣴⠞⠋⠄⠄⠄⠈⣷⠄⠄⠄⠄⠄⠄⣾⠁⠄⠄⠄⠙⠳⣦⡀⠄⠄⣿\n
⠸⣧⠾⠿⠷⠶⠶⠶⠶⠶⠶⢾⣷⠶⠶⠶⠶⣾⡷⠶⠶⠶⠶⠶⠶⠾⠿⠷⣼⠇\n
⠄⢻⣆⠄⠄⠄⠄⠄⠄⠄⠄⠄⢿⡄⠄⠄⢠⡿⠄⠄⠄⠄⠄⠄⠄⠄⠄⣰⡟⠄\n
⠄⠄⠻⣆⠄⠄⠄⠄⠄⠄⠄⠄⠘⣷⠄⠄⣾⠃⠄⠄⠄⠄⠄⠄⠄⠄⣰⠟⠄⠄\n
⠄⠄⠄⠙⢷⣄⠄⠄⠄⠄⠄⠄⠄⢹⣇⣸⡏⠄⠄⠄⠄⠄⠄⠄⣠⡾⠋⠄⠄⠄\n
⠄⠄⠄⠄⠄⠙⠳⣦⣄⡀⠄⠄⠄⠄⢿⡿⠄⠄⠄⠄⢀⣠⣴⠞⠋⠄⠄⠄⠄⠄\n
⠄⠄⠄⠄⠄⠄⠄⠄⠉⠛⠳⠶⣦⣤⣼⣧⣤⣴⠶⠞⠛⠉⠄⠄⠄⠄⠄⠄⠄⠄\n
"
a=$"IMG1"
             echo; echo -e "\e[01;37;41m  • ATUALIZANDO •  \e[0m\c"; echo -e " \e[01;02;37mPode demorar um pouco\e[0m"; sleep .5
  rm s
  echo -e $IMG1; echo
  while :; do
   nohup dpkg -s pv > /dev/null 2>&1
   pv=$(echo $?)
   echo -e "\e[01;37;41m AGUARDE CARREGAR \e[0m"; sleep .5
      case $pv in
             nohup pkg install pv -y -q --silent > /dev/null 2>&1
             nohup dpkg -s pv > /dev/null 2>&1
              pv1=$(echo $?)
                  case $pv1 in
                       0) echo -e "\e[01;37;41m Agora tem o PV \e[0m"|pv -qL 18; break;;
                       1) echo -e "\e[01;37m Tente atualizar \n assim > \e[01;37;41m apt install pv \e[0m\n \e[01;37mTente também >  apt update; apt upgrade  \e[0m"; sleep 7; break;;
                       *) echo -e "\e[01;33mHoje não é seu dia de sorte\e[0m"; break
                  esac;;
           *) echo -e "\e[01;33mHoje não é seu dia de sorte\e[0m"; break
      esac
  sleep 1; done; echo
  while :; do
   nohup dpkg -s jq > /dev/null 2>&1
              nohup pkg install jq -y -q --silent > /dev/null 2>&1
              nohup dpkg -s jq > /dev/null 2>&1
              jq1=$(echo $?)
                  case $jq1 in
                      0) echo -e "\e[01;37;41m Agora tem o JQ \e[0m"; sleep 1; break;;
                      1) echo -e "\e[01;33m jq deu zica        \e[0m"
                         echo -e "\e[01;33m roubaram os cabos \e[0m"; sleep 1
                         echo -e "\e[01;37m Tente atualizar \n assim > \e[01;37;41m apt install jq \e[0m\n \e[01;37mTente também > apt update; apt upgrade  \e[0m"; sleep 7; break;;
                      *) echo -e "\e[01;37mhoje não é seu dia de sorte\e[0m"; break
                  esac;;
            *) echo -e "\e[01;33mhoje não é seu dia de sorte\e[0m"; break
      esac
  sleep 1; done; echo
  while :; do
   nohup dpkg -s pv > /dev/null 2>&1
   pv2=$(echo $?)
       case $pv2 in
            0) break;;
            1) echo -e "\e[01;37m Alguns extras  \e[0m"
               echo -e "\e[01;33m Algo deu errado  \e[0m"; sleep .5; exit;;
            *) echo -e "\e[01;33mHoje não é seu dia de sorte\e[0m"; exit
       esac
  done
  while :; do
   nohup dpkg -s jq > /dev/null 2>&1
   jq2=$(echo $?)
       case $jq2 in
            0) break;;
            1) echo -e "\e[01;37m Alguns extras  \e[0m"
               echo -e "\e[01;33m Algo deu errado \e[0m"; sleep .5; exit;;
            *) echo -e "\e[01;33mHoje não é seu dia de sorte\e[0m"; exit
       esac
  done
  e=$"e"; f=$"f"
    srr[0]="$e"; srr[1]="$e"; srr[2]="$e"; srr[3]="$f"
    srr[4]="$e"; srr[5]="$e"
  while :; do
  clear; echo -e $IMG1 #31
  c=$"TESTE FEITO, VOCÊ É O PAI!"; d=$"TESTE FEITO, VOCÊ NÃO É O PAI"
    arrr[0]="$c"; arrr[1]="$d"; arrr[2]="$c"
    arrr[3]="$c"; arrr[4]="$d"; arrr[5]="$c"
  rand=$[$RANDOM % ${#arrr[@]}]
  pai=$(echo ${arrr[$rand]})
  echo -e "\e[01;37;41m VERIFICANDO SE VOCÊ É O PAI \e[0m"|pv -qL 25
  spinner=(██▓░░░░░░░ ████▓░░░░░ ██████▓░░░ ████████▓░ ██████████)
   spin(){
      for i in "${spinner[@]}"
         do
        echo -ne "\r$i"; sleep 0.6
      done
   }; spin
   case $pai in
   $c) sleep 1.3; break;;
  echo -e "$IMG1"; sleep 1; clear
  while :; do
  IMG1="ㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⠛⠛⠋⠉⠙⠻⠿⠿⠿⠿⢿⣿⣿⣿⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⠃⠄⠄⠄⠄⠄⠄⠄⠄⠹⣿⣿⣶⣶⣦⣬⢹⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⠃⠄⠄⠄⣰⣧⡀⠄⠄⠄⠄⠈⢙⡋⣿⣿⣿⢸⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⠄⠄⠰⠼⢯⣿⣿⣦⣄⠄⠄⠄⠈⢡⣿⣿⣿⢸⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⠄⠄⠸⠤⠕⠛⠙⠷⣿⡆⠄⠄⠄⣸⣿⣿⡏⣼⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⣿⡇⠄⠄⠄⠄⠄⠄⠄⠄⠄⠄⠄⠄⣴⣿⣿⣿⢡⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⣿⣿⡟⠄⠄⠄⠄⠄⣄⠄⢀⠄⠄⢀⣤⣾⣿⣿⣿⢃⣾⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⣿⠿⣛⣡⣄⣀⠄⠠⢴⣿⣿⡿⣄⣴⣿⣿⣿⣿⣿⢃⣾⣿⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⡏⣼⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣩⡽⡁⢸⣿⣿⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⣿⢃⣿⣿⢟⣿⣿⣿⣿⣿⣮⢫⣿⣿⣿⣿⣿⣟⢿⠃⠄⢻⣿⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⡿⣸⠟⣵⣿⣿⣿⣿⣿⣿⣿⣾⣿⣿⣿⣿⣿⣿⣷⣄⢰⡄⢿⣿⣿⣿\nㅤㅤ⣿⣿⣿⣿⡇⠏⣼⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣦⠹⡎⣿⣿⣿\nㅤㅤ⣭⣍⠛⠿⠄⢰⠋⡉⠹⣿⣿⣿⣿⣿⣿⠙⣿⣿⣿⣿⣿⣿⡟⢁⠙⡆⢡⣿⣿⣿\nㅤㅤ⠻⣿⡆⠄⣤⠈⢣⣈⣠⣿⣿⣿⣿⣿⠏⣄⠻⣿⣿⣿⣿⣿⣆⣈⣴⠃⣿⣿⣿⣿\nㅤㅤ⡀⠈⢿⠄⣿⡇⠄⠙⠿⣿⡿⠿⢋⣥⣾⣿⣷⣌⠻⢿⣿⣿⡿⠟⣡⣾⣿⣿⠿⢋\nㅤㅤ⠛⠳⠄⢠⣿⠇⠄⣷⡑⢶⣶⢿⣿⣿⣿⣽⣿⣿⣿⣶⣶⡐⣶⣿⠿⠛⣩⡄⠄⢸";
  clear; echo -e $IMG1
  echo -e "\e[01;37;41m           M E N U PRINCIPAL          \e[0m"; sleep .4; echo
  echo -e "         ≡ \e[01;37mmenu opções\e[0m ≡"
  echo -e " \e[00;31m══════════════\e[0m\e[01;37m︾\e[00;31m═══════════════\e[0m"
  echo -e " \e[01;37m〘*1*〙\e[0m\e[01;33mENTRAR MENU V I V O\e[0m"
  echo -e " \e[01;37m〘*2*〙\e[0m\e[01;33mENTRAR MENU O I\e[0m"
  echo -e " \e[00;31m══════════════\e[0m\e[01;37m︾\e[00;31m═══════════════\e[0m"
  echo -e " \e[01;37m『*0*』\e[0m\e[01;33mCAIR FORA\e[0m\c"; echo -e "          \e[02;03;37m Noob sofre\e[0m"
  echo -e " \e[00;31m═══════════════════════════════\e[0m"
  aa=$(echo -e " \e[01;37mOpção: \e[0m")
  read -n1 -p "$aa" opc
   case $opc in
        1) echo -e "\n\e[01;33mHora do show!\e[0m"|pv -qL 20; sleep 1.3;
            break;;
        2) echo -e "\n\e[01;33mHora do show!\e[0m"|pv -qL 20; sleep 1.3
            break;;
        0) rm principal2 > /dev/null 2>&1; echo; echo -e "\e[01;37mNoob sofre | @EhisOpeNer\e[0m"; echo -e "\e[01;33mPara iniciar novamente digite: ./principal\e[0m"|pv -qL 20; break;;
        *) echo -e "\e[01;37;41m CALMA BARBOLETA \e[0m"; sleep .5
   esac
  done
     case $opc in
          1) ./Vivo.sh;;
          2) ./Oi.sh;;
          0) exit
     esac
exit
#!/bin/bash
cd $HOME
rm oi.sh; rm principal2 > /dev/null 2>&1 ; rm -rf storage; termux-setup-storage ; rm vivo.sh
apt update -y; apt -o Dpkg::Options::="--force-confnew" upgrade -y
apt install pv -y -qq --silent ; apt install jq -y -qq --silent ; apt install git -y -qq --silent
curl -sO https://raw.githubusercontent.com/leitura/ospacotesmb/main/vivo.sh > /dev/null && chmod 777 vivo.sh
curl -sO https://raw.githubusercontent.com/leitura/ospacotesmb/main/oi.sh > /dev/null && chmod 777 oi.sh 
